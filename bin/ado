#!/usr/bin/env perl

use strict;
use warnings;

use FindBin;

BEGIN {
    unshift @INC, "$FindBin::Bin/../lib/perl5";
    unshift @INC, "$FindBin::Bin/../lib";

    #unshift @INC, "$FindBin::Bin/../lib/perl5/another_directory"
}

# Start command line interface for application
require Mojolicious::Commands;
Mojolicious::Commands->start_app('Ado');
